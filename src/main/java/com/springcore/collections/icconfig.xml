<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://springframework.org/schema/context" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.springcore.collections.Emp" name="Emp">
		<property name="name" value="Panguni"></property>
		<property name="phones">
			<list>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
				<value>5</value>
				<null/>
			</list>
		</property>
		<property name="addresses">
			<set>
				<value>PA</value>
				<value>NJ</value>
				<value>TX</value>
				<value>NC</value>
				<null></null>			
			</set>
		</property>
		<property name="courses">
			<map>
				<entry key="java" value="2months"/>
				<entry key="python" value="1months"/>
				<entry key="c" value=".5months"/>
			
			</map>
		
		
		</property>
		<property name="qualities">
			<props>
				<prop key="Temper">Cool</prop>
				<prop key="Height">Normal</prop>
			
			</props>
		
		</property>
		
	</bean>

</beans>